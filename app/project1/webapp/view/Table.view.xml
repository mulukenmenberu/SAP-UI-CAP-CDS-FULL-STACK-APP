<mvc:View 	controllerName="project1.controller.Table"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	xmlns:form="sap.ui.layout.form"
	xmlns="sap.m"
	xmlns:m="sap.m"
	xmlns:table="sap.ui.table"
	xmlns:l="sap.ui.layout"

>

	
	<table:Table selectionMode="None" rows="{/Rows}">
		<table:columns>
			<table:Column width="10rem">
				<m:Label text="Student DB ID"/>
				<table:template>
					<m:Label text="{Supplier}"/>
				</table:template>
			</table:Column>
			<table:Column width="10rem">
				<m:Label text="Full Name"/>
				<table:template>
					<m:Label text="{Supplier ID}"/>
				</table:template>
			</table:Column>
			<table:Column width="10rem">
				<m:Label text="Member Date"/>
				<table:template>
					<m:Label text="{Sales Manager}"/>
				</table:template>

			</table:Column>
			<table:Column width="10rem">
				<m:Label text="App Type"/>
				<table:template>
					<m:Label text="{Product ID}"/>
				</table:template>
			</table:Column>
			<table:Column width="10rem">
				<m:Label text="Count"/>
				<table:template>
					<m:Label text="{Unit Price}"/>
				</table:template>
			</table:Column>
			<table:Column width="10rem">
				<m:Label text="Message"/>
				<table:template>
					<m:Label text="{Period}"/>
				</table:template>
			</table:Column>
			<table:Column >
				<m:Label text="Status"/>
				<table:template>
				
				<HBox class="vBoxContainer">
					<!-- <template:if Supplier="{= ${Supplier} === 1}">
					<template:then>
						<Text text="{Supplier}" tooltip="{Supplier}" wrapping="true" class="crmStatusOk" />
					</template:then>
					<template:else>
						<Text text="{Supplier}" tooltip="{Supplier}" wrapping="true" class="crmDefault" />
							</template:else>
					</template:if> -->
                        <items>
						<ColumnListItem vAlign="Middle">
							<cells>
								<ObjectIdentifier title="{Supplier}" text="{Supplier}" />
								<Text text="{Supplier}" />
								<Text text="{Supplier}" />
							</cells>
						</ColumnListItem>
					</items>
	                          <VBox class="crmStatusOk">
									<Text
										text="CS"
										class="overlayText"
									/>
								</VBox>


								<VBox class="crmStatusOk">
									<Text
										text="AP"
										class="overlayText"
									/>
								</VBox>
								<VBox class="crmStatusOk">
									<Text
										text="DC"
										class="overlayText"
									/>
								</VBox>
								<VBox class="crmDefault">
									<Text
										text="FC"
										class="overlayText"
									/>
								</VBox>
								<VBox class="crmDefault" >
									<Text
										text="LF"
										class="overlayText"
									/>
								</VBox>
							</HBox>
				</table:template>				
			</table:Column>
		</table:columns>
	</table:Table>
</mvc:View>


